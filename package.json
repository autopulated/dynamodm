{
  "name": "dynamodm",
  "version": "1.6.0",
  "description": "Simple Document mapper for DynamoDB, inspired by mongoose.",
  "main": "index.js",
  "scripts": {
    "test": "tap run",
    "lint": "./node_modules/.bin/eslint -c eslint.config.js './{,!(node_modules)/**/}*.js' './{,!(node_modules)/**/}*.mjs'"
  },
  "tap": {
    "show-full-coverage": true,
    "allow-incomplete-coverage": true,
    "coverage-report": [
      "text",
      "lcovonly"
    ],
    "files": [
      "test"
    ],
    "timeout": 60000
  },
  "files": [
    "index.js",
    "lib/*.js"
  ],
  "keywords": [
    "dynamodb",
    "dynamodb-doc-client",
    "dynamo",
    "document",
    "mapper",
    "mongoose"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/autopulated/dynamodm.git"
  },
  "bugs": {
    "url": "https://github.com/autopulated/dynamodm/issues"
  },
  "homepage": "https://github.com/autopulated/dynamodm",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.848.0",
    "@aws-sdk/lib-dynamodb": "^3.848.0",
    "abstract-logging": "^2.0.1",
    "ajv": "^8.17.1",
    "bson-objectid": "^2.0.4",
    "pino": "^9.13.1"
  },
  "author": "James Crosby <3456211+autopulated@users.noreply.github.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "eslint": "^9.37.0",
    "globals": "^16.3.0",
    "tap": "^21.1.0"
  }
}
